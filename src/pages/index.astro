---
import LandingPageHtml from '@layouts/LandingPageHtml.astro';
import { Navbar } from '@components/Navbar';
import { Hero } from '@components/LandingPage/Hero';
import { Partners } from '@components/LandingPage/Partners';
import AnnouncementModal from '@components/AnnouncementModal';
import { AgentsVideo } from '@components/LandingPage/AgentsVideo';
import { IdeaToAgentSteps } from '@components/LandingPage/IdeaToAgentSteps';
import { Templates } from '@components/LandingPage/Templates';
import Footer from '@components/Footer';
import { PricingCalculator } from '@components/LandingPage/PricingCalculator';
import { Faq } from '@components/LandingPage/Faq';
import { Features } from '@components/LandingPage/Features';
import { LLMs } from '@components/LandingPage/LLMs';
import { Benefits } from '@components/LandingPage/Benefits';
import { Integrations } from '@components/LandingPage/Integrations';
import { SecurityInMind } from '@components/LandingPage/SecurityInMind';
import { MultiUse } from '@components/LandingPage/MultiUse';
import { CaseStudies } from '@components/LandingPage/CaseStudies';
import { Testimonials } from '@components/LandingPage/Testimonials';
import DeployOnFleek from '@components/LandingPage/DeployOnFleek';
import ImpressUsers from '@components/LandingPage/ImpressUsers';
import ExplainerBlocks from '@components/LandingPage/ExplainerBlocks';
import PricingLanding from '@components/LandingPage/PricingLanding';
import BuildUseCases from '@components/LandingPage/BuildUseCases';
import ReadyToLive from '@components/LandingPage/ReadyToLive';
---

<LandingPageHtml>
  <div
    class="pointer-events-none absolute left-0 top-0 h-[50vw] w-full bg-[radial-gradient(ellipse_60%_60%_at_top,#6E6E6E40_0%,transparent_100%)] sm:h-[30vw]"
  >
  </div>
  <AnnouncementModal pathname={Astro.url.pathname} client:only="react" />
  <Navbar pathname={Astro.url.pathname} variant="fixed" client:load />
  <Hero client:load />
  <Partners />
  <DeployOnFleek />
  <ImpressUsers client:load />
  <ExplainerBlocks />
  <PricingLanding client:load />
  <BuildUseCases />
  <Faq client:load />
  <ReadyToLive />
  {
    /*<Partners />
  <AgentsVideo />
  <IdeaToAgentSteps />
  <Templates client:load />
  <Features />
  <LLMs />
  <Benefits />
  <Integrations client:load />
  <MultiUse />
  <Testimonials />
  <CaseStudies />
  <SecurityInMind />
  <PricingCalculator client:load />
  */
  }
  <Footer />
</LandingPageHtml>
