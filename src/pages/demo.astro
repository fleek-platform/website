---
import LandingPageHtml from '@layouts/LandingPageHtml.astro';
import { Navbar } from '@components/Navbar';
import { Hero } from '@components/LandingPage/Hero';
import { Partners } from '@components/LandingPage/Partners';
import DeployOnFleek from '@components/LandingPage/DeployOnFleek';
import ImpressUsers from '@components/LandingPage/ImpressUsers';
import BuildUseCases from '@components/LandingPage/BuildUseCases';
import ExplainerBlocks from '@components/LandingPage/ExplainerBlocks';
import PricingLanding from '@components/LandingPage/PricingLanding';
import ReadyToLive from '@components/LandingPage/ReadyToLive';
import Footer from '@components/Footer';
import AnnouncementModal from '@components/AnnouncementModal';
import { FaCircleArrowUp } from "react-icons/fa6";
import { ChatOrDeployDemo } from '@components/ChatToAIAgentDeploy';
---

<LandingPageHtml>
  <Navbar pathname={Astro.url.pathname} client:load />
  <ChatOrDeployDemo
    client:load
    fleekRestApi={import.meta.env.PUBLIC_FLEEK_REST_API_URL}
    personagenEndpoint={import.meta.env.PUBLIC_PERSONAGEN_ENDPOINT}
  />
  <Footer />
</LandingPageHtml>
